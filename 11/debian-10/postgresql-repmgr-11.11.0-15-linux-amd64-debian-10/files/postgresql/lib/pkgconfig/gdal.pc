CONFIG_VERSION=3.3.0
CONFIG_INST_PREFIX=/opt/qingcloud/postgresql
CONFIG_INST_LIBS=-L/opt/qingcloud/postgresql/lib -lgdal
CONFIG_INST_CFLAGS=-I/opt/qingcloud/postgresql/include
CONFIG_INST_DATA=/opt/qingcloud/postgresql/share/gdal
prefix=${CONFIG_INST_PREFIX}
